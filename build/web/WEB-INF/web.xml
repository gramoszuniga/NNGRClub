<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>NNGRDisplayBooksServlet</servlet-name>
        <servlet-class>club.admin.NNGRDisplayBooksServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NNGRAddBookServlet</servlet-name>
        <servlet-class>club.admin.NNGRAddBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NNGRLoanServlet</servlet-name>
        <servlet-class>club.cart.NNGRLoanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NNGRCartServlet</servlet-name>
        <servlet-class>club.cart.NNGRCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NNGRClearCartServlet</servlet-name>
        <servlet-class>club.cart.NNGRClearCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NNGRMemberAdminController</servlet-name>
        <servlet-class>club.admin.NNGRMemberAdminController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NNGRDisplayBooksServlet</servlet-name>
        <url-pattern>/NNGRDisplayBooks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NNGRAddBookServlet</servlet-name>
        <url-pattern>/NNGRAddBook</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NNGRLoanServlet</servlet-name>
        <url-pattern>/NNGRLoan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NNGRCartServlet</servlet-name>
        <url-pattern>/NNGRCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NNGRClearCartServlet</servlet-name>
        <url-pattern>/NNGRClearCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NNGRMemberAdminController</servlet-name>
        <url-pattern>/NNGRMemberAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>NNGRIndex.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <exception-type>java.lang.Throwable</exception-type>
        <location>/NNGRError.jsp</location>
    </error-page>
</web-app>
